.footerTop .col .action_button{background-color: #ffffaa; margin: 0; border-radius: 0; padding: 0 15px; line-height: 35px; height: 35px; font-family: 'Univers Condensed'; color: #646464; font-size: 20px; text-transform: uppercase;}
.footerTop .col .action_button:hover{color: #ffffaa;}
.socialMain{padding:10px 0 20px;}
.socialMain a{display: inline-block; vertical-align: middle; padding-right: 20px;}
.socialMain a:hover{opacity: 0.5;}
.copyRightMain {background-color: #ddebf9; padding: 10px 0; }
