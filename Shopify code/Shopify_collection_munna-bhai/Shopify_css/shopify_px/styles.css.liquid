.instaMain h2 a{padding-left: 40px; background: url("insta_icon.png") no-repeat left center; color: #646464;}
.instaMain img{max-width: 100%; display: inline-block;}
.footerTop {padding: 60px 0 40px; background: url("footer_bg.png") repeat-x top left;}
.footerTop .col {float: left; width: 20%; padding-right: 30px;}
