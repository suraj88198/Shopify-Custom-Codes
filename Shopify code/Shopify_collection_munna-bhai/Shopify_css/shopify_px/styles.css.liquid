.TweetsMain .Tweets::before{position: absolute; height: 100%;  width: 1200px; background-color: #fff; content: ""; right:0px; -webkit-box-shadow: 2px 2px 5px 0px rgba(33,30,30,0.3);
-moz-box-shadow: 2px 2px 5px 0px rgba(33,30,30,0.3);
box-shadow: 2px 2px 5px 0px rgba(33,30,30,0.3); z-index: -1; top:-5px;}
